{
  "name": "cheery-scrapio",
  "version": "1.0.0",
  "description": "A news article scraper that aggregates articles from different political perspectives",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "audit": "npm audit",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JimGray9999/cheery-scrapio.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JimGray9999/cheery-scrapio/issues"
  },
  "homepage": "https://github.com/JimGray9999/cheery-scrapio#readme",
  "dependencies": {
    "axios": "^1.13.1",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-handlebars": "^8.0.3",
    "express-validator": "^7.3.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "mongoose": "^8.19.2",
    "nodemon": "^3.1.10",
    "winston": "^3.19.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "supertest": "^7.2.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,handlebars}": [
      "prettier --write"
    ]
  }
}
